{
  "manifest_version": 2,
  "name": "Autofill Form Extension",
  "description": "Form autofill on steroids",
  "version": "1.0.0",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "<all_urls>",
		"tts",
		"storage",
		"unlimitedStorage",
		"contextMenus",
		"webNavigation",
    "tabs",
    "activeTab",
    "notifications",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "icon-16.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/content.js"]
    }
  ],
  "commands": {
		"new_template": {
      "suggested_key": {
        "default": "Ctrl+1",
        "mac": "MacCtrl+1"
      },
			"description": "New Template"
    }
  },
  "content_security_policy": "script-src 'self' 'sha256-GgRxrVOKNdB4LrRsVPDSbzvfdV4UqglmviH9GoBJ5jk='; object-src 'self'"
}